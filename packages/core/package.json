{
  "name": "@sourcer/core",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "test": "vitest run",
    "cover": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.5.2",
    "typescript": "^5.9.3",
    "vitest": "^1.5.2"
  }
}
